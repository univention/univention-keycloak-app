  "tags": [
    {
      "description": "User objects",
      "name": "users/user"
    }
  ],
  "components": {
    "schemas": {
      "users-user": {
        "type": "object",
        "properties": {
          "dn": {
            "description": "LDAP DN (Distinguished Name)",
            "type": "string",
            "format": "dn",
            "example": "dc=example,dc=net",
            "readOnly": true
          },
          "uuid": {
            "description": "LDAP Entry-UUID",
            "type": "string",
            "format": "uuid"
          },
          "objectType": {
            "description": "UDM Object-Type",
            "type": "string",
            "example": "users/user",
            "readOnly": true
          },
          "_links": {
            "description": "Hypertext Application Language (HAL) links",
            "type": "object",
            "properties": {
              "self": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "href": {
                      "type": "string",
                      "description": "The URL"
                    }
                  },
                  "additionalProperties": true
                }
              }
            },
            "readOnly": true,
            "additionalProperties": true
          },
          "_embedded": {
            "description": "Hypertext Application Language (HAL) embedded resources",
            "type": "object",
            "properties": {},
            "readOnly": true,
            "additionalProperties": true
          },
          "position": {
            "description": "DN of LDAP node below which the object is located. Changing this causes a move of the object. When chaning no other changes are applied.",
            "type": "string",
            "format": "dn",
            "example": "cn=position,dc=example,dc=net"
          },
          "properties": {
            "type": "object",
            "properties": {
              "username": {
                "type": "string",
                "nullable": true
              },
              "uidNumber": {
                "type": "integer",
                "nullable": true
              },
              "gidNumber": {
                "type": "integer",
                "nullable": true
              },
              "firstname": {
                "type": "string",
                "nullable": true
              },
              "lastname": {
                "type": "string",
                "nullable": true
              },
              "gecos": {
                "type": "string",
                "nullable": true
              },
              "displayName": {
                "type": "string",
                "nullable": true
              },
              "title": {
                "type": "string",
                "nullable": true
              },
              "initials": {
                "type": "string",
                "nullable": true
              },
              "preferredDeliveryMethod": {
                "type": "string",
                "nullable": true
              },
              "sambaPrivileges": {
                "type": "array",
                "nullable": true,
                "items": {
                  "type": "string",
                  "nullable": true
                },
                "uniqueItems": true
              },
              "description": {
                "type": "string",
                "nullable": true
              },
              "organisation": {
                "type": "string",
                "nullable": true
              },
              "userexpiry": {
                "type": "string",
                "format": "date",
                "nullable": true
              },
              "passwordexpiry": {
                "type": "string",
                "format": "date",
                "nullable": true
              },
              "pwdChangeNextLogin": {
                "type": "boolean",
                "nullable": true
              },
              "preferredLanguage": {
                "type": "string",
                "nullable": true
              },
              "disabled": {
                "type": "boolean",
                "nullable": true
              },
              "locked": {
                "type": "boolean",
                "nullable": true
              },
              "lockedTime": {
                "type": "string",
                "nullable": true
              },
              "unlock": {
                "type": "boolean",
                "nullable": true
              },
              "unlockTime": {
                "type": "string",
                "nullable": true
              },
              "password": {
                "type": "string",
                "format": "password",
                "example": "univention",
                "readOnly": true,
                "nullable": true
              },
              "street": {
                "type": "string",
                "nullable": true
              },
              "e-mail": {
                "type": "array",
                "nullable": true,
                "items": {
                  "type": "string",
